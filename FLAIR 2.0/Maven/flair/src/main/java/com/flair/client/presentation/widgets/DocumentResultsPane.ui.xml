<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:m.html="urn:import:gwt.material.design.client.ui.html"
	xmlns:flair="urn:import:com.flair.client.presentation.widgets">
		
	<ui:style>
	.global-animation {
		transition: 0.4s all;
		-webkit-transition: 0.4s all;
		-moz-transition: 0.4s all;
	}
	</ui:style>
	
	<m:MaterialPanel ui:field="pnlRootUI" depth="700" backgroundColor="WHITE" overflow="AUTO" 
		grid="l6 m6 s12" height="100%" padding="15" top="68"
		addStyleNames="{style.global-animation}" textAlign="CENTER">
		<m:MaterialTitle ui:field="lblTitleUI" marginBottom="20" title="" description=""/>
		
		<m:MaterialPanel ui:field="pnlCompletedContainerUI" padding="10" marginBottom="10"/>
		
		<m:MaterialDivider ui:field="divDividerUI" visible="false"/>
		
		<m:MaterialRow ui:field="pnlSpinnerUI" visible="false">
			<m:MaterialPreLoader size="BIG" marginTop="5">
				<m:MaterialSpinner color="BLUE_ONLY"/>
			</m:MaterialPreLoader>
		</m:MaterialRow>
		
		<m:MaterialPanel ui:field="pnlInProgressContainerUI" padding="10" marginTop="20"/>
	</m:MaterialPanel>
</ui:UiBinder> 