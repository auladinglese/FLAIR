							<flair:GrammaticalConstructionPanelItem ui:field="pnlImperativeUI" text="imperatives" iconType="EXPAND_MORE">
									<flair:GrammaticalConstructionWeightSlider gram="IMPERATIVES"/>
							</flair:GrammaticalConstructionPanelItem>
						</flair:GrammaticalConstructionPanel>
					</flair:GrammaticalConstructionPanelItem>
				
					<!-- Negation -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNegationUI" text="negation" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_ALL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_PARTIAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NO_NOT_NEVER"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NOT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NT"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Articles -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlArticlesUI" text="articles" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLES"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_A"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_AN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_THE"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Quantifiers -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlQuantifiersUI" text="quantifiers" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_SOME"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_ANY"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_MUCH"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_MANY"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Adjectives -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlAdjectivesUI" text="adjectives" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_POSITIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_COMPARATIVE_SHORT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_SUPERLATIVE_SHORT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_COMPARATIVE_LONG"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_SUPERLATIVE_LONG"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Adverbs -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlAdverbsUI" text="adverbs" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_POSITIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_COMPARATIVE_SHORT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_SUPERLATIVE_SHORT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_COMPARATIVE_LONG"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_SUPERLATIVE_LONG"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Pronouns -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPronounsUI" text="pronouns" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_SUBJECTIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_OBJECTIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_POSSESSIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_POSSESSIVE_ABSOLUTE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_REFLEXIVE"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Conjunctions -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlConjunctionsUI" text="conjunctions" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="CONJUNCTIONS_SIMPLE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CONJUNCTIONS_ADVANCED"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Prepositions -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPrepositionsUI" text="prepositions" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS_SIMPLE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS_COMPLEX"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Nouns -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNounsUI" text="nouns" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PLURAL_REGULAR"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PLURAL_IRREGULAR"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NOUNFORMS_ING"/>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>	
		</flair:GrammaticalConstructionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 