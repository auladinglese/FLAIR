<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:flair="urn:import:com.flair.client.presentation.widgets">
	
	<g:HTMLPanel ui:field="pnlRootUI">
		<flair:GrammaticalConstructionPanel accordion="true">			
			<!-- Sentences -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlSentencesUI" text="sentences" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="true">
					<!-- Questions -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlQuestionsUI" text="questions" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_DIRECT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WH"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_TO_DO"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_TO_BE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_TO_HAVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_YESNO"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_TAG"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Sentence Types -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlSentenceTypesUI" text="sentence types" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_SIMPLE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_COMPOUND"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_SUBORDINATE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_INCOMPLETE"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Clause Types -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlClauseTypesUI" text="clause types" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_RELATIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_ADVERBIAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CONDITIONALS_REAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CONDITIONALS_UNREAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="THERE_IS_ARE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="THERE_WAS_WERE"/>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>
			
			<!-- Parts of Speech -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlPartsOfSpeechUI" text="parts of speech" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="true">
					<!-- Verbs -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbsUI" text="verbs" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionPanel accordion="true" popout="true">
							<!-- Verb Forms -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbFormsUI" text="verb forms" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_SHORT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_LONG"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_AUXILIARIES_BE_DO_HAVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_COPULAR"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_ING"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_TO_INFINITIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_EMPATHIC_DO"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBS_IRREGULAR"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBS_REGULAR"/>
							</flair:GrammaticalConstructionPanelItem>			
			
							<!-- Tenses -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlTensesUI" text="tenses" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_SIMPLE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PROGRESSIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_SIMPLE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PROGRESSIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_SIMPLE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_PROGRESSIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PERFECT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PERFECT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_PERFECT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PERFECT_PROGRESSIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PERFECT_PROGRESSIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_PERFECT_PROGRESSIVE"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Aspect -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlAspectUI" text="aspect" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_SIMPLE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_PROGRESSIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_PERFECT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_PERFECT_PROGRESSIVE"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Time -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlTimeUI" text="time" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="TIME_PRESENT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TIME_PAST"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TIME_FUTURE"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Voice -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVoiceUI" text="voice" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="PASSIVE_VOICE"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Phrasal -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlPhrasalUI" text="phrasal" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERBS_PHRASAL"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Modal -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlModalUI" text="modal" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="MODALS_SIMPLE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="MODALS_ADVANCED"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Transitive -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlTransitiveUI" text="transitive" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="OBJECT_DIRECT"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="OBJECT_INDIRECT"/>
							</flair:GrammaticalConstructionPanelItem>
							
