<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:m.html="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
	.card {
		margin-top: 10px;
		margin-left: 10px;
		margin-right: 10px;
		
		padding: 10px;
	}
	</ui:style>
	
	<m:MaterialCard ui:field="pnlCardUI" addStyleNames="{style.card}">
		<m:MaterialRow grid="l6 m6 s8">
			<m:MaterialColumn ui:field="colCompletedHeaderUI" grid="l1 m1 s2" textAlign="CENTER">
				<m:MaterialLabel ui:field="lblNewRankUI" fontSize="3.0em" text="1"/>				
				<m:MaterialIcon ui:field="icoRankOffsetUI" iconSize="SMALL" iconColor="GREY_LIGHTEN_2" iconType="KEYBOARD_ARROW_UP"/>
			</m:MaterialColumn>
			
			<m:MaterialColumn ui:field="colInProgressHeaderUI" grid="l1 m1 s3" textAlign="CENTER">
				<m:MaterialPreLoader size="BIG" marginTop="5">
					<m:MaterialSpinner color="BLUE_ONLY"/>
				</m:MaterialPreLoader>
			</m:MaterialColumn>
			
			<m:MaterialColumn grid="l5 m5 s6">
				<m:MaterialLink ui:field="btnTitleUI" iconColor="BLACK" fontSize="1.5em" 
					text="My back hurts!"/>
					
				<m:MaterialLabel ui:field="lblURLUI" textColor="GREY_LIGHTEN_1" fontSize="0.8em" 
					text="www.imsodonewiththis.com"/>
			
				<m:MaterialLabel ui:field="lblSnippetUI" paddingTop="10" 
					text="Rabble Rabble Rabble"/>
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialCard>
</ui:UiBinder> 