<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:flair="urn:import:com.flair.client.presentation.widgets">
	
	<g:HTMLPanel ui:field="pnlRootUI">
		<flair:GrammaticalConstructionPanel accordion="true">
			<!-- S채tze -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlSaetzeUI" text="s채tze" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="true">
					<!-- Fragen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlFragenUI" text="fragen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_DIRECT"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WH"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_YESNO"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Satztypen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlSatztypenUI" text="sentence types" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_SIMPLE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_COMPOUND"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_SUBORDINATE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_INCOMPLETE"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Nebensatztypen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNebensatztypenUI" text="nebensatztypen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_RELATIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_ADVERBIAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_DASS"/>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>
		
			<!-- Wortarten -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlWortartenUI" text="wortarten" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="true">
					<!-- Verben -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbenUI" text="verben" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionPanel accordion="true" popout="true">
							<!-- Verbtypen -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbtypenUI" text="verbtypen" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERBTYP_MAIN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBTYP_AUXILIARIES"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBTYP_MODAL"/>
							</flair:GrammaticalConstructionPanelItem>			
			
							<!-- Zeiten -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlZeitenUI" text="zeiten" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PERFECT_HABEN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PERFECT_SEIN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PERFECT_HABEN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PERFECT_SEIN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_SIMPLE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_PERFECT"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Passiv -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlPassivUI" text="passiv" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="PASSIVE_VOICE_WERDEN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="PASSIVE_VOICE_SEIN"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Satzklammer -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlSatzklammerUI" text="satzklammer" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERB_BRACKETS"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERB_CLUSTER"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Verbformen -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbformenUI" text="verbformen" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="IMPERATIVES"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_INFINITIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_TO_INFINITIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_PARTICIPLE"/>
							</flair:GrammaticalConstructionPanelItem>
						</flair:GrammaticalConstructionPanel>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Negation -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNegationUI" text="negation" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_ALL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_PARTIAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NO_NOT_NEVER"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Artikel -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlArtikelUI" text="artikel" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLES"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_A"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_THE"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Indefinite Quantifizierer -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlIndefiniteQuantifiziererUI" text="indefinite quantifizierer" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_SOME"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_ANY"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_MANY"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Adjektive, Adverben und Zahlworte -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlAdjAdvZWUI" text="adjektive, adverben und zahlworte" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_POSITIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_POSITIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PARTICLE_PLUS_ADJ_ADV"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CARDINALS"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Pronomen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPronomenUI" text="pronomen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_PERSONAL"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_RELATIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_POSSESSIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_DEMONSTRATIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_REFLEXIVE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_INDEFINITE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_INTERROGATIVE"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Pr채positionen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPraepositionenUI" text="pr채positionen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS_SIMPLE"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS_COMPLEX"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Nomen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNomenUI" text="nomen">
						<flair:GrammaticalConstructionPanel accordion="true" popout="true">
							<!-- Nominalisierungen -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlNominalisierungenUI" text="nominalisierungen" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="NOUNS_ISMUS"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="NOUNS_TUR"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="NOUNS_UNG"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Attribute -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlAttributeUI" text="attribute" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_PARTICIPLE_1"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_PARTICIPLE_2"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_ADJECTIVE"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_PREPOSITION"/>
							</flair:GrammaticalConstructionPanelItem>
						</flair:GrammaticalConstructionPanel>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>
		</flair:GrammaticalConstructionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 