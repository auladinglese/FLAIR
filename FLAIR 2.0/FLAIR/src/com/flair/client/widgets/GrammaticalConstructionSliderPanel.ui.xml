<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

	<b:PanelGroup>
		<b:Panel ui:field="pnlWrapperUI" type="DEFAULT">
			<b:PanelHeader ui:field="pnlHeaderUI">
				<b:Heading size="H4">
					<b:Anchor ui:field="btnHeaderTextUI" dataToggle="COLLAPSE"/>
					<b:Anchor ui:field="btnResetUI" icon="ERASER" styleName="close"/>
				</b:Heading>
			</b:PanelHeader>
			
			<b:PanelCollapse ui:field="pnlCollapseUI">
				<b:PanelBody ui:field="pnlBodyUI"/>
			</b:PanelCollapse>
		</b:Panel>
	</b:PanelGroup>
	
</ui:UiBinder> 