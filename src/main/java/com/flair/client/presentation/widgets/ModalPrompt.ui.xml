<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
>
	
	<ui:style>
	
	</ui:style>

    <m:MaterialDialog ui:field="mdlPromptUI" type="BOTTOM_SHEET" dismissible="false">
        <m:MaterialDialogContent marginTop="13">
			<m:MaterialTitle ui:field="lblTextUI" title="Title" description="Description"
				marginTop="13"/>
        </m:MaterialDialogContent>

        <m:MaterialDialogFooter>
			<m:MaterialButton ui:field="btnYesUI" type="FLAT" text="Yes"/>
			<m:MaterialButton ui:field="btnNoUI" type="FLAT" text="No"/>
        </m:MaterialDialogFooter>
    </m:MaterialDialog>
</ui:UiBinder> 