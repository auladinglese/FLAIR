<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
>
	<ui:style>
	
	</ui:style>

    <m:MaterialDialog ui:field="mdlExporterUI" dismissible="true">
        <m:MaterialDialogContent>
			<m:MaterialRow textAlign="CENTER">
				<m:MaterialTitle ui:field="lblTitleUI" title="Export Settings" 
					description="You can use this link to apply your current settings to further searches, or share the link with students so they can use the web search with your settings."/>
			</m:MaterialRow>
			
			<m:MaterialRow>
				<m:MaterialTextBox ui:field="txtURLUI" text="Exported_URL" readOnly="false"/>
			</m:MaterialRow>
        </m:MaterialDialogContent>

        <m:MaterialDialogFooter>
			<m:MaterialButton ui:field="btnCloseUI" type="FLAT" text="Close"/>
        </m:MaterialDialogFooter>
    </m:MaterialDialog>
</ui:UiBinder> 