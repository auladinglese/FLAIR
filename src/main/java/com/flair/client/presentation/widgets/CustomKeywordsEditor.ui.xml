<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
>
	
	<ui:style>
	
	</ui:style>

    <m:MaterialDialog ui:field="mdlKeywordEditorUI" dismissible="false">
        <m:MaterialDialogContent>
			<m:MaterialRow textAlign="CENTER">
				<m:MaterialTitle ui:field="lblTitleUI" 
					title="Edit Custom Vocabulary" description=""/>
			</m:MaterialRow>
			
			<m:MaterialRow>
				<m:MaterialTextArea ui:field="txtKeywordsUI" 
					label="Enter your keywords here, separated by commas or line breaks."/>
			</m:MaterialRow>
        </m:MaterialDialogContent>

        <m:MaterialDialogFooter>
			<m:MaterialButton ui:field="btnApplyUI" type="FLAT" text="Apply"/>
			<m:MaterialButton ui:field="btnCancelUI" type="FLAT" text="Cancel"/>
        </m:MaterialDialogFooter>
    </m:MaterialDialog>
</ui:UiBinder> 